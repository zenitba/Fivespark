<%- include('./partials/head.ejs') %>
<!-- basis -->
<% for( let i = 0; i < selectedContracts.length; i++ ) { %>
  <section id="services" aria-label="services">
    <div class="services">
      <div class="pricing-table">
        <div class="head">
          <h3 class="title"><%- selectedContractTranslations[i].title %></h3>
        </div>
        <div class="content">
          <div class="price">
            <h4>â‚¬<%- selectedContracts[i].list_price %></h4>
          </div>
          <ul>
            <% activityTranslations.data.forEach(activityTranslation => { %>
              <% const activity = activityTranslation.activity; %>
              <li><i class="fa fa-check" aria-hidden="true"></i>
                <%- activityTranslation.title %>
              </li>
            <% }); %>
          </ul>
          <div class="sign-up">
            <a href="#" class="btn bordered radius">Selecteer deze plan</a>
          </div>
        </div>
      </div>
    </div>
  </section>
	<% } %>


  


